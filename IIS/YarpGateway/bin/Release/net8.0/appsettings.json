{
  "ReverseProxy": {
    "Routes": {
      "flask": {
        "ClusterId": "flaskCluster",
        "Match": { "Path": "{**catch-all}" }
      }
    },
    "Clusters": {
      "flaskCluster": {
        "Destinations": {
          "d1": { "Address": "http://127.0.0.1:5001/" }
        }
      }
    }
  }
}
